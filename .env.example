# HealthFlow AI Environment Configuration

# OpenAI API Configuration
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_OPENAI_MODEL=gpt-4

# Twilio SMS Configuration
VITE_TWILIO_ACCOUNT_SID=your_twilio_account_sid
VITE_TWILIO_AUTH_TOKEN=your_twilio_auth_token
VITE_TWILIO_PHONE_NUMBER=your_twilio_phone_number

# SendGrid Email Configuration
VITE_SENDGRID_API_KEY=your_sendgrid_api_key
VITE_SENDGRID_FROM_EMAIL=noreply@healthflow-ai.com

# Application Configuration
VITE_APP_NAME=HealthFlow AI
VITE_APP_VERSION=1.0.0
VITE_API_BASE_URL=http://localhost:3001/api

# Database Configuration (for future backend implementation)
DATABASE_URL=postgresql://username:password@localhost:5432/healthflow_ai
JWT_SECRET=your_jwt_secret_key_here

# Rate Limiting
VITE_RATE_LIMIT_REQUESTS_PER_MINUTE=60
VITE_RATE_LIMIT_REQUESTS_PER_HOUR=1000
